{
  "name": "gulp-starter-kit",
  "version": "1.2.0",
  "description": "![Banner](./banner.png)",
  "main": "index.js",
  "scripts": {
    "scss:build": "node-sass src/sass/styles.scss -o src/css",
    "scss:watch": "node-sass --watch src/sass/styles.scss -o src/css",
    "build": "npm run scss:build && npm run scss:watch",
    "start": "cross-env PORT=1234 gulp start --development ",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "author": "Isoiev Kamil <isoyevkm@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "autoprefixer": "^9.8.0",
    "browser-sync": "^2.27.10",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "gh-pages": "^2.2.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-cache-bust": "^1.4.1",
    "gulp-concat": "^2.6.1",
    "gulp-group-css-media-queries": "^1.2.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-inject": "^5.0.5",
    "gulp-mode": "^1.0.2",
    "gulp-newer": "^1.4.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-rigger": "^0.5.8",
    "gulp-sass": "^4.1.0",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "require-dir": "^1.2.0",
    "usedcss": "^1.0.7"
  },
  "dependencies": {
    "node-sass": "^4.14.1"
  }
}
